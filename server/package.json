  "version": "1.0.0",
  "description": "Tampere Vuorovaikutusalusta API Server",
  "main": "src/app.js",
  "scripts": {
    "dev": "concurrently npm:dev:* --color",
    "dev:server": "nodemon src/app",
    "dev:test": "jest --watch",
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/app",
    "migrate": "node-pg-migrate -f ./migration.config.js --template-file-name ./migrations/.template.ts",
    "test": "jest --config ./jest.config.json",
    "lint": "eslint . --ext .ts",
    "prebuild": "npm run lint && npm run test",
    "build": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": "prettier --write --ignore-unknown"
